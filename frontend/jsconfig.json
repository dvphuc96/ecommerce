{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@/*": ["src/*"],
            "@components/*": ["src/components/*"],
            "@assets/*": ["src/assets/*"],
            "@constant/*": ["src/constant/*"],
            "@hooks/*": ["src/hooks/*"],
            "@pages/*": ["src/pages/*"],
            "@guards/*": ["src/guards/*"],
            "@router/*": ["src/router/*"],
            "@schema/*": ["src/schema/*"],
            "@services/*": ["src/services/*"],
            "@types/*": ["src/types/*"],
            "@utils/*": ["src/utils/*"],
            "@store/*": ["src/store/*"],
        },
    },
    "include": ["src/**/*"],
}